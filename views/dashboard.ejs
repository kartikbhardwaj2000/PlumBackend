<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Dashboard</title>
    <style>
        li:nth-child(1) {
            font-weight: bold;
        }
    </style>
</head>
<body>
    <h1>Top links Dashboard</h1>
    <h2>Top Domains</h2>
    <ol class="bold">
        <% domains.forEach(function(domainObj) { %>
          <li>
             <%= domainObj.domain %> was shared <%= domainObj.frequency %> times
          </li>
        <% }); %>
      </ol>
      <h2>Top link sharing users</h2>
    <ol class="bold">
        <% users.forEach(function(user) { %>
          <li>
             <%= user.name %> shared <%=user.linksCount %> links
          </li>
        <% }); %>
      </ol>
    <h2>tweets</h2>
    <ol>
        <% tweets.forEach(function(tweet) { %>
          <li>
             <%= tweet %>
          </li>
        <% }); %>
    </ol>
    
    

</body>
</html>